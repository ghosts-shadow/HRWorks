                                    if (totalworkingdays < 1095)
                                    {
                                        eos = (totalworkingdays * 7) / 365;
                                    }
                                    else if (totalworkingdays >= 1095 && totalworkingdays < 1825)
                                    {
                                        var eos_3 = (1095 * 7) / 365;
                                        var eos3_5 = ((totalworkingdays - 1095) * 14) / 365;
                                        eos = eos3_5 + eos_3;
                                    }
                                    else
                                    {
                                        eos = (105 + ((totalworkingdays - 1825) * 30) / 365);
                                    }
