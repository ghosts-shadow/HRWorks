@model IEnumerable<HRworks.Models.formsuserdb>

@{
    ViewBag.Title = "formsdashboard";
    var empno = Model.First().master_file.employee_no;
    var empname = Model.First().master_file.employee_name;
}

<h2>submitted forms</h2>
<div class="row">
    <label class="control-label col-md-2">
        employee no 
    </label>
    <div class="col-md-10">
        :@empno
    </div> 
</div>

<div class="row">
    <label class="control-label col-md-2">
        employee name
    </label>
    <div class="col-md-10">
        :@empname
    </div> 
</div>
<div class="row">
    <div class="col-md-offset-2 col-md-10">
        <input class="btn btn-default" type="button" onclick="window.location.href = '@Url.Action("newFormssubmission", "Forms")'" value="New form submission" />
    </div>
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.reqdate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Form.form_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.reqdate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Form.form_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @*@Html.ActionLink("view", "", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id=item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
        </td>
    </tr>
}

</table>
