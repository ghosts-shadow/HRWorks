@using Microsoft.Ajax.Utilities
@model IEnumerable<HRworks.Models.Leave>

@{
    ViewBag.Title = "duplive";
}

<h2>duplive</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.master_file.employee_no)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.master_file.employee_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Start_leave)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.End_leave)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Return_leave)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.leave_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.actual_return_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.submitted_by)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.approved_by)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.apstatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.half)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.actualchangedby)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.actualchangeddateby)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @if (item.master_file.emiid.IsNullOrWhiteSpace())
            {
                @Html.DisplayFor(modelItem => item.master_file.employee_no)
            }
            else
            {
                @Html.DisplayFor(modelItem => item.master_file.emiid)
            }
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.master_file.employee_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Start_leave)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.End_leave)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Return_leave)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.leave_type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.actual_return_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.submitted_by)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.approved_by)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.apstatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.half)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.actualchangedby)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.actualchangeddateby)
        </td>
    </tr>
}

</table>
